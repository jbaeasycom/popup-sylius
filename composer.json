{
    "name": "workouse/popup-sylius",
    "type": "sylius-plugin",
    "keywords": ["sylius", "sylius-plugin"],
    "description": "Popup plugin Bundle for Sylius E-Commerce.",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "sylius/sylius": "v1.13",
        "friendsofsymfony/ckeditor-bundle": "^2.0"
    },
    "require-dev": {
        "behat/behat": "^3.6.1",
        "behat/mink-selenium2-driver": "^1.6",
        "dmore/behat-chrome-extension": "^1.3",
        "dmore/chrome-mink-driver": "^2.7",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/variadic-extension": "^1.3",
        "friends-of-behat/mink-debug-extension": "^2.1",
        "phpspec/phpspec": "7.4.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-webmozart-assert": "^1.2",
        "phpunit/phpunit": "^9.0",
        "sylius-labs/coding-standard": "^4.3",
        "symfony/browser-kit": "v6.4",
        "symfony/debug-bundle": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/intl": "^6.4",
        "symfony/web-profiler-bundle": "^6.4",
        "vimeo/psalm": "5.13.1",
        "sebastian/recursion-context": "^4.0",
        "symfony/console": "^6.4"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Workouse\\PopupPlugin\\": "src/",
            "Tests\\Workouse\\PopupPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/Kernel.php"]
    },
    "config": {
        "allow-plugins": {
            "symfony/thanks": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
